    "fp-ts": "^1.0.1",
    "history": "^4.7.2",
    "io-ts": "^1.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rxjs": "^5.5.4"
  },
  "peerDependencies": {
    "@types/history": "^4.6.2"
