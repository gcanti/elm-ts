    "prebuild": "rm -rf lib",
    "build": "tsc -p ./tsconfig.build.json",
    "fix-prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
