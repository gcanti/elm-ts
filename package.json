{
  "name": "elm-ts",
  "version": "0.4.3",
  "description": "A porting of TEA to TypeScript featuring fp-ts, io-ts, rxjs5 and React",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "mocha": "TS_NODE_CACHE=false mocha -r ts-node/register test/*.ts",
    "test": "npm run lint && npm run mocha && npm run docs",
    "build": "rm -rf lib/* && tsc",
    "fix-prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --write \"{src,test}/**/*.ts\"",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/elm-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/elm-ts/issues"
  },
  "homepage": "https://github.com/gcanti/elm-ts",
  "dependencies": {
    "axios": "^0.19.0",
    "fp-ts": "^2.1.1",
    "history": "^4.10.1",
    "io-ts": "^2.0.1",
    "react": "^16.11.0",
    "rxjs": "^6.5.3"
  },
  "peerDependencies": {
    "@types/history": "^4.7.3"
  },
  "devDependencies": {
    "@types/blessed": "0.1.12",
    "@types/history": "4.7.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.11.6",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "awesome-typescript-loader": "^5.2.1",
    "blessed": "0.1.81",
    "docs-ts": "0.0.1",
    "mocha": "^6.2.2",
    "monocle-ts": "^2.0.0",
    "prettier": "^1.18.2",
    "react-blessed": "^0.6.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2"
  },
  "tags": [
    "typescript",
    "elm",
    "fp-ts"
  ],
  "keywords": [
    "typescript",
    "elm",
    "fp-ts"
  ],
  "engines": {
    "node": "12"
  }
}
