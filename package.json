    "react": "^16.8.6",
    "react-blessed": "^0.5.0",
    "rxjs": "^6.5.2",
    "ts-node": "3.3.0",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3",
