  "bugs": {
    "url": "https://github.com/gcanti/elm-ts/issues"
  },
  "homepage": "https://github.com/gcanti/elm-ts",
  "dependencies": {
    "axios": "^0.17.1",
    "history": "^4.7.2"
  },
  "peerDependencies": {
    "@types/history": "^4.6.2",
    "fp-ts": "^2.0.2",
    "io-ts": "^2.0.0",
    "rxjs": "^6.5.2",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@types/blessed": "0.1.8",
    "@types/history": "4.6.2",
